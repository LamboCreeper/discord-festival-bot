  "dependencies": {
    "discord.js": "^14.7.1",
    "discordx": "^11.7.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "cross-env": "^7.0.3",
